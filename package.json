    "@actions/core": "^1.6.0"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/node": "^13.9.3",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vercel/ncc": "^0.32.0",
    "autoprefixer": "^9.7.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
